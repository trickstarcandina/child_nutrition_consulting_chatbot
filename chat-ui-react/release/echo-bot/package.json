{
  "name": "echo-bot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.5.0",
    "@emotion/styled": "11.3.0",
    "@mui/material": "5.0.4",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.0",
    "@testing-library/user-event": "13.2.1",
    "@twihike/eslint-config": "0.1.17",
    "@twihike/prettier-config": "0.1.17",
    "@twihike/stylelint-config": "0.1.17",
    "@types/jest": "27.0.1",
    "@types/node": "16.9.2",
    "@types/react": "17.0.21",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "chat-ui-react": "0.3.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.25.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "36.0.0",
    "husky": "4.3.8",
    "lint-staged": "11.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "stylelint": "13.13.1",
    "typescript": "4.4.3",
    "web-vitals": "2.1.0"
  },
  "scripts": {
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier --check src",
    "format": "run-s format:*",
    "format:eslint": "eslint --fix src",
    "format:prettier": "prettier --write src",
    "test": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts test",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,mjs,jsx,ts,tsx}": "eslint",
    "*.{css}": "stylelint"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@twihike"
  },
  "stylelint": {
    "extends": "@twihike/stylelint-config"
  },
  "prettier": "@twihike/prettier-config",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
